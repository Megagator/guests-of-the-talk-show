{
  "name": "guests-of-the-talk-show",
  "version": "1.0.0",
  "private": true,
  "description": "Guests of the Talk Show",
  "author": "Mike Reinhard",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop -p 8585",
    "start": "gatsby develop -p 8585",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "prettier --write . --loglevel=warn",
    "check": "prettier --check .",
    "build:fetch": "npm run lint && tsc && mkdir -p dist/utilities && cp src/utilities/slugify.js dist/utilities",
    "parse": "node dist/feed/parse.js",
    "deploy": "npm run check && gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "gatsby": "^4.11.1",
    "gatsby-plugin-no-index": "^1.0.2",
    "luxon": "^2.3.0",
    "node-fetch": "^2.6.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^8.0.2",
    "rehype-raw": "^6.1.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "gh-pages": "^3.2.3",
    "prettier": "2.6.1",
    "typescript": "^4.5.5"
  }
}
